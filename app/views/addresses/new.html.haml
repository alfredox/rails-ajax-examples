= form_for @address do |f|
  %fieldset
    %legend Address
    = f.label :street
    = f.text_field :street
    %br
    = f.label :no_ext
    = f.text_field :no_ext
    %br
    = f.label :zip_code
    = f.text_field :zip_code, :class => 'zip_code_autocomplete'
    %br
    = f.label :city
    = f.text_field :city
    %br
    = f.label :country
    = f.text_field :country
    %br
    = f.label :no_int
    = f.select :no_int, ['Int A', 'Int B']
    %br
    = f.submit "Submit"
